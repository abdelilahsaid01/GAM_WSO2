<?xml version="1.0" encoding="UTF-8"?>
<proxy name="API_Authentification_Proxy" startOnLoad="true" transports="http https" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <log level="custom">
                <property expression="json-eval($)" name="API_Authentification_Proxy: Sending Request To Back End"/>
            </log>
            <send>
                <endpoint key="API_Auehtification_Endpoints"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </target>
</proxy>
