<?xml version="1.0" encoding="UTF-8"?>
<sequence name="SendToLogQueue" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <!--     <property name="messageType" scope="axis2" type="STRING" value="application/json"/> -->
    <!--     <property name="ContentType" scope="axis2" type="STRING" value="application/json"/> -->
    <property name="OUT_ONLY" scope="default" type="STRING" value="true"/>
    <property name="FORCE_SC_ACCEPTED" scope="axis2" type="STRING" value="true"/>
    <send>
        <endpoint>
            <address uri="jms:/in_queue?transport.jms.ConnectionFactoryJNDIName=QueueConnectionFactory&amp;java.naming.factory.initial=org.wso2.andes.jndi.PropertiesFileInitialContextFactory&amp;java.naming.provider.url=conf/jndi.properties&amp;transport.jms.ConnectionFactoryType=queue">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>1</progressionFactor>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </address>
        </endpoint>
    </send>
</sequence>
